r <- aggregate(r, fact=factor.ratio, fun=sum, na.rm=TRUE)
# 5. Select only the shape of Bangladesh as defined by the base layer
r <- mask(x = r, mask = base.raster)
# Assign a name to the raster
names(r) <- label
return(r)
}
